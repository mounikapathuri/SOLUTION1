package solution1;
import java.util.Scanner;
import java.io.IOException;
/**
 *
 * @author mounika
 */
public class aliquote{
 public static void main(String[] args) {
         long sum=0;
		for(long num=1;num<100;num++){
		
		for(long i=1;i<=num/2;i++){
			
			if(num%i==0){
				sum=sum+i;
			}
		}
		if(num==sum){
			 System.out.println(num+" is a perfect number");
			 sum=0;
		}
		else{
			sum=0;
		}
		
		}
        
  }
    }
